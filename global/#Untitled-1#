rm diagnostictool.tgz && sudo rm -rf /home/diagnostictool/diagnostictool && sudo -u diagnostictool mkdir /home/diagnostictool/diagnostictool && wget -O diagnostictool.tgz "http://nexus.cengage.info/service/local/artifact/maven/redirect?r=analytics-snapshots&g=com.cengage.analytics&a=diagnostictool_2.11&v=1.0.0-SNAPSHOT&e=tgz" && sudo tar -C /home/diagnostictool/diagnostictool -xzf diagnostictool.tgz --strip-components 1 && sudo chown -R diagnostictool:diagnostictool /home/diagnostictool && sudo supervisorctl restart diagnostictool 
